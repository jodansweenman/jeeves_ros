
<launch>
  <node pkg="ros_pololu" type="ros_pololu_node.py" name="ros_pololu" output="screen">
        <param name="pololu_motors_yaml" value="$(find finger_math)/launch/pololu_motors.yaml" />
        <param name="port_name" value="/dev/pololu"/>
        <param name="baud_rate" value="115200" />
        <param name="rate_hz" value="10" />
        <param name="daisy_chain" value="false" />
        <param name="controller" value="Maestro" />
    </node>
</launch>
