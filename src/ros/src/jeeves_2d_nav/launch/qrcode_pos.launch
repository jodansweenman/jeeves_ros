<launch>
  <!-- Launch the freenect node -->
  <include file="$(find freenect_launch)/launch/freenect.launch" />

  <!-- Launch the qrcode_pos service -->
  <node name="qrcode_pos" pkg="qrcode_pos" type="qrcode_pos_service.py">
    <param name="timeout_value" value="5" />
    <param name="project" value="mcecs_jeeves" />
  </node>
</launch>

