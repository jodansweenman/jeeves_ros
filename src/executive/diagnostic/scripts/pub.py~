a = 0	



def adder():
	
	rospy.init_node('pub', anonymous = True)
	
	rospy.Subscriber('data1',FeedbackA, value_a_callback)
	rospy.loginfo(a)	
	rospy.Subscriber('data2',FeedbackB, value_b_callback)
	rospy.loginfo(b)	
	rate = rospy.Rate(50)
	#rate.sleep()
	while not rospy.is_shutdown():
		a = 22
		pub = rospy.Publisher('data1', FeedbackA, queue_size = 5)
		#rospy.loginfo(a)
		pub.publish(FeedbackA(a))
		rate.sleep()

	#rospy.spin()



if __name__ == '__main__':
	adder()

